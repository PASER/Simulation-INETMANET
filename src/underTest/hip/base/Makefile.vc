	@echo This means that at least the above dependency directory has not been built.
	@echo Maybe you need to do a top-level make?
	@echo.
	@exit /b 1

subdirs: $(SUBDIR_TARGETS)

HipMessages_m.obj: HipMessages_m.cc
	$(CXX) -c $(NEDCOPTS) /Tp HipMessages_m.cc

HipMessages_m.cc HipMessages_m.h: HipMessages.msg
	$(MSGC:/=\) -s _m.cc $(MSGCOPTS) HipMessages.msg

HIP.obj: HIP.cc
	$(CXX) -c $(COPTS) /Tp HIP.cc

HipFsm.obj: HipFsm.cc
	$(CXX) -c $(COPTS) /Tp HipFsm.cc

HipFsmBase.obj: HipFsmBase.cc
	$(CXX) -c $(COPTS) /Tp HipFsmBase.cc

RvsHIP.obj: RvsHIP.cc
	$(CXX) -c $(COPTS) /Tp RvsHIP.cc


